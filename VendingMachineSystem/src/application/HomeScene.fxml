<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SceneController">
   <left>
      <Pane prefHeight="600.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="72.0" layoutY="252.0" mnemonicParsing="false" onAction="#switchToCustomerScene" prefHeight="61.0" prefWidth="116.0" text="Customer">
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="12.0" />
               </font>
            </Button>
            <Button layoutX="242.0" layoutY="252.0" mnemonicParsing="false" onAction="#switchToTechnicianScene" prefHeight="61.0" prefWidth="116.0" text="Technician">
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="12.0" />
               </font>
            </Button>
            <Button layoutX="412.0" layoutY="252.0" mnemonicParsing="false" onAction="#switchToManagerScene" prefHeight="61.0" prefWidth="116.0" text="Manager">
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="12.0" />
               </font>
            </Button>
            <Label layoutX="150.0" layoutY="83.0" prefHeight="100.0" prefWidth="300.0" text="        Welcome to SnackAttack" textFill="#1c6b4c">
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="21.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </left>
</BorderPane>
